{# twig template #}
<div class="modal fade" id="vacuum-vid-modal" tabindex="-1" role="dialog" aria-labelledby="vacuum-vid-modal-title" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="vacuum-vid-modal-title">The SkyVac Gutter Vacuum</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
